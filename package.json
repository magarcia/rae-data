{
	"name": "rae-extractor",
	"version": "0.0.1",
	"description": "Extracts the list of words and definitions from https://www.rae.es/",
	"main": "dist/main.js",
	"type": "module",
	"scripts": {
		"build": "tsc",
		"start": "node --experimental-specifier-resolution=node --loader ts-node/esm src/main.ts",
		"lint": "eslint src/**/*.ts",
		"test": "jest"
	},
	"keywords": [],
	"author": "Martin Garcia <newluxfero@gmail.com>",
	"license": "MIT",
	"xo": {
		"rules": {
			"quote-props": "consistent"
		}
	},
	"prettier": {
		"singleQuote": true,
		"useTabs": true,
		"arrowParens": "avoid",
		"semi": true,
		"trailingComma": "all",
		"bracketSpacing": false,
		"quoteProps": "consistent"
	},
	"devDependencies": {
		"@types/cache-manager-fs-hash": "^0.0.1",
		"@types/debug": "^4.1.7",
		"@types/jest": "^29.4.0",
		"@types/lockfile": "^1",
		"@types/proper-lockfile": "^4",
		"@types/xo": "^0",
		"@typescript-eslint/eslint-plugin": ">=5.43.0",
		"@typescript-eslint/parser": ">=5.43.0",
		"eslint": ">=8.0.0",
		"eslint-config-semistandard": "latest",
		"eslint-config-standard": "latest",
		"eslint-config-xo": "latest",
		"eslint-config-xo-typescript": "latest",
		"eslint-plugin-import": "^2.25.2",
		"eslint-plugin-n": "^15.0.0",
		"eslint-plugin-promise": "^6.0.0",
		"husky": "^8.0.3",
		"jest": "^29.5.0",
		"ts-jest": "^29.0.5",
		"typescript": ">=4.4",
		"xo": "^0.53.1"
	},
	"dependencies": {
		"cache-manager": "^5.1.7",
		"cache-manager-fs-hash": "^1.0.0",
		"cheerio": "^1.0.0-rc.12",
		"debug": "^4.3.4",
		"got": "^12.6.0",
		"got-cached": "^1.2.1",
		"keyv": "^4.5.2",
		"listr2": "^5.0.8",
		"lockfile": "^1.0.4",
		"prettier": "^2.8.4",
		"proper-lockfile": "^4.1.2",
		"ts-node": "^10.9.1"
	},
	"repository": "git@github.com:magarcia/rae-data.git",
	"private": true
}
